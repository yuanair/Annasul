{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo test 'annasul'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--all-features",
                    "--package=annasul"
                ],
                "env": {
                    "RUST_LOG": "info",
                    "RUST_BACKTRACE": "1"
                }
            },
            "args": [
                "--nocapture"
            ]
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Cargo launch 'example'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=example",
                    "--package=example"
                ],
                "env": {
                    "RUST_LOG": "info",
                    "RUST_BACKTRACE": "1"
                }
            },
            "args": []
        }
    ]
}