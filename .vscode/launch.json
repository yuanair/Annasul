{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "example: Windows, Debug",
            "preLaunchTask": "example: build debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/example.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/target/debug",
            "environment": [
                {
                    "name": "RUST_LOG",
                    "value": "info"
                },
                {
                    "name": "RUST_BACKTRACE",
                    "value": "1"
                }
            ],
            "console": "integratedTerminal"
        }
    ]
}