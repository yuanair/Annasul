{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "doc",
			"args": [
				"--no-deps"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "cargo doc"
		},
		{
			"type": "cargo",
			"command": "doc",
			"args": [
				"--no-deps",
				"--open"
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "cargo doc --open"
		}
	]
}